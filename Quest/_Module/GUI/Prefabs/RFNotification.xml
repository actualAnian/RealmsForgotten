<Prefab>
  <Window>
    <Widget MarginLeft="10" MarginRight="10" MarginTop="-15" MarginBottom="10" WidthSizePolicy="StretchToParent" HeightSizePolicy="StretchToParent">
      <Children>
        <Standard.Window Parameter.Title="@TitleLabel">
          <Children>
            <TextWidget MarginTop="50" Brush.FontSize="@RFFontSize" WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Center" Text="@TextLabel" />
            <ImageWidget PositionYOffset="50" PositionXOffset="650" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" VerticalAlignment="Center" HorizontalAlignment="Center" SuggestedWidth="2500" SuggestedHeight="700" Sprite="prisoner_image"/>
          </Children>
        </Standard.Window>
        <RealmsForgotten.DialogLeaveButton Parameter.DoneButtonAction="ExecuteDone" Parameter.DoneButtonText="@DoneLabel" />
      </Children>
    </Widget>
  </Window>
</Prefab>
